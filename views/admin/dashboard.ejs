<div class="admin-title">
<h2>Posts</h2>
<a herf="/add-post" class="button">+ Add New</a>
</div>

<ul class="admin-posts">

    <% data.forEach(post => %>

    <li>
        <a herf="/post/<%= post._id %>">
        <%= post.title %> &nearr;
        </a>
<div class="admin-post-controls">
    <a herf="/edit-post/<%= post._id %>" class="btn">Edit</a>

    <form action="/delete-post/<%= data._id %>?_method=DELETE" method="POST">
        <input type="submit" value="Delete" class="btn">
    </form>
</div>


    </li>



    <%})%>


</ul>